{
    "name": "ng-seed",
    "version": "5.3.0",
    "description": "New template",
    "repository": "https://github.com/alexrudol/ng-seed",
    "license": "MIT",
    "config": {
        "protocol": "http",
        "localhost": "localhost",
        "localhost_port": 1111
