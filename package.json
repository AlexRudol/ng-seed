        "angular2-template-loader": "~0.6.2",
        "rimraf": "~2.6.0",
        "superstatic": "~5.0.0",
        "webpack": "~4.5.0",
        "webpack-cli": "~2.0.0",
        "webpack-dev-server": "~3.1.0",
        "webpack-shell-plugin": "~0.5.0",
        "webpack-merge": "~4.1.0",
        "@types/core-js": "~0.9.46",
        "@types/node": "~9.6.0",
        "awesome-typescript-loader": "~5.0.0",
        "codelyzer": "~4.2.0",
        "to-string-loader": "~1.1.0",
