        "angular2-template-loader": "~0.6.2",
        "rimraf": "~2.6.2",
        "superstatic": "~5.0.1",
        "webpack": "~3.8.1",
        "webpack-dev-server": "~2.9.4",
        "webpack-shell-plugin": "~0.5.0",
        "webpack-merge": "~4.1.1",
        "@types/core-js": "~0.9.43",
        "@types/node": "~8.0.53",
        "awesome-typescript-loader": "~3.4.0",
        "codelyzer": "~4.0.1",
        "to-string-loader": "~1.1.5",
        "tslint": "~5.8.0",
