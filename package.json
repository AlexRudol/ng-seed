{
    "name": "ng-seed",
    "version": "6.4.0",
    "description": "New template",
    "repository": "https://github.com/alexrudol/ng-seed",
    "license": "MIT",
    "config": {
        "protocol": "http",
        "localhost": "localhost",
        "localhost_port": 1111
    },
    "scripts": {
        "clean": "rimraf dist && rimraf node_modules",
        "clean:soft": "rimraf dist",
