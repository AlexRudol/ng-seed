{
    "name": "ng-seed",
    "version": "0.2.0",
    "description": "New template",
    "repository": "https://github.com/alexrudol/ng-seed",
    "license": "MIT",
    "config": {
        "protocol": "http",
        "localhost": "localhost",
        "localhost_port": 1111
    },
    "scripts": {
        "clean": "rimraf node_modules dist && npm cache clean",
        "clean:build": "rimraf dist",
        "server": "superstatic --config config/superstatic.json dist/ --gzip",
        "start": "webpack-dev-server --progress --inline --hot --config config/webpack.dev.js",
        "lint": "tslint -c config/tslint.json \"src/app/**/*.ts\"",
        "prebuild": "rimraf dist",
        "build": "webpack --progress --config config/webpack.prod.js"
    },
    "devDependencies": {
        "@angular/platform-server": "4.4.6",
        "@ngtools/webpack": "~1.8.0",
        "@types/core-js": "~0.9.43",
        "@types/node": "~8.0.50",
        "@types/uglify-js": "~2.6.29",
        "@types/webpack": "~3.8.0",
        "angular2-router-loader": "~0.3.5",
        "angular2-template-loader": "~0.6.2",
        "awesome-typescript-loader": "~3.3.0",
        "compression-webpack-plugin": "~1.0.1",
        "html-webpack-plugin": "~2.30.1",
        "optimize-js-plugin": "~0.0.4",
        "raw-loader": "~0.5.1",
        "rimraf": "~2.6.2",
        "superstatic": "~5.0.1",
        "ts-lint": "~4.5.1",
        "typescript": "~2.6.1",
        "uglifyjs-webpack-plugin": "~1.0.1",
        "webpack": "~3.8.1",
        "webpack-dev-server": "~2.9.4",
        "webpack-merge": "~4.1.1"
    },
    "dependencies": {
        "@angular/animations": "4.4.6",
        "@angular/common": "4.4.6",
        "@angular/compiler": "4.4.6",
        "@angular/compiler-cli": "4.4.6",
        "@angular/core": "4.4.6",
        "@angular/platform-browser": "4.4.6",
        "@angular/platform-browser-dynamic": "4.4.6",
        "core-js": "~2.5.1",
        "rxjs": "~5.5.2",
        "zone.js": "~0.8.18"
    }
}
